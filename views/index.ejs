<!DOCTYPE html>
<html lang="en">
<head>
	
	<%- include('fragmentos/head') %> 

</head>
<body>
	
	<!--PreLoader-->
    <div class="loader">
        <div class="loader-inner">
            <div class="circle"></div>
        </div>
    </div>
    <!--PreLoader Ends-->

	<!-- header -->
	<%- include('fragmentos/header') %> 
	
	<!-- end header -->

	<!-- search area -->
	<div class="search-area">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<span class="close-btn"><i class="fas fa-window-close"></i></span>
					<div class="search-bar">
						<div class="search-bar-tablecell">
							<h3>Search For:</h3>
							<input type="text" placeholder="Keywords">
							<button type="submit">Search <i class="fas fa-search"></i></button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end search area -->

	<!-- hero area -->
	<div class="hero-area hero-bg">
		<div class="container">
			<div class="row">
				<div class="col-lg-9 offset-lg-2 text-center">
					<div class="hero-text">
						<div class="hero-text-tablecell">
							<p class="subtitle">RDRENTA</p>
							<h1>Desplazate a tu gusto</h1>
							<div class="hero-btns">
								<a href="shop.html" class="boxed-btn">Colección</a>
								<a href="contact.html" class="bordered-btn">Contact</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end hero area -->

	<!-- features list section -->
	<div class="list-section pt-80 pb-80">
		<div class="container">

			<div class="row">
				<div class="col-lg-4 col-md-6 mb-4 mb-lg-0">
					<div class="list-box d-flex align-items-center">
						<div class="list-icon">
							<i class="fas fa-shipping-fast"></i>
						</div>
						<div class="content">
							<h3>Free Shipping</h3>
							<p>When order over $75</p>
						</div>
					</div>
				</div>
				<div class="col-lg-4 col-md-6 mb-4 mb-lg-0">
					<div class="list-box d-flex align-items-center">
						<div class="list-icon">
							<i class="fas fa-phone-volume"></i>
						</div>
						<div class="content">
							<h3>24/7 Support</h3>
							<p>Get support all day</p>
						</div>
					</div>
				</div>
				<div class="col-lg-4 col-md-6">
					<div class="list-box d-flex justify-content-start align-items-center">
						<div class="list-icon">
							<i class="fas fa-sync"></i>
						</div>
						<div class="content">
							<h3>Refund</h3>
							<p>Get refund within 3 days!</p>
						</div>
					</div>
				</div>
			</div>

		</div>
	</div>
	<!-- end features list section -->

	<!-- product section -->
	<div class="product-section mt-150 mb-150">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 offset-lg-2 text-center">
					<div class="section-title">	
						<h3><span class="orange-text">Más</span> rentados</h3>
						<!--<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Aliquid, fuga quas itaque eveniet beatae optio.</p>
						-->
						</div>
				</div>
			</div>

			<div class="row">
				<div class="col-lg-4 col-md-6 text-center">
					<div class="single-product-item">
						<div class="product-image">
							<a href="single-product.html"><img src="resources/assets/img/carros/vento.jpg" alt=""></a>
						</div>
						<h3>Vento</h3>
						<p class="product-price"><span>Por día</span> 800$ </p>
						<a href="cart.html" class="cart-btn"><i class="fas fa-shopping-cart"></i> Apartar</a>
					</div>
				</div>
				<div class="col-lg-4 col-md-6 text-center">
					<div class="single-product-item">
						<div class="product-image">
							<a href="single-product.html"><img src="resources/assets/img/carros/versa.jpg" alt=""></a>
						</div>
						<h3>Versa</h3>
						<p class="product-price"><span>Por día</span> 800$ </p>
						<a href="cart.html" class="cart-btn"><i class="fas fa-shopping-cart"></i> Apartar</a>
					</div>
				</div>
				<div class="col-lg-4 col-md-6 offset-md-3 offset-lg-0 text-center">
					<div class="single-product-item">
						<div class="product-image">
							<a href="single-product.html"><img src="resources/assets/img/carros/crv.jpg" alt=""></a>
						</div>
						<h3>Crv</h3>
						<p class="product-price"><span>Por día</span> 1200$ </p>
						<a href="cart.html" class="cart-btn"><i class="fas fa-shopping-cart"></i> Apartar</a>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end product section -->

	



	
	<!-- shop banner -->
	<section class="shop-banner">
    	<div class="container">
        	<h3>December sale is on! <br> with big <span class="orange-text">Discount...</span></h3>
            <div class="sale-percent"><span>Sale! <br> Upto</span>50% <span>off</span></div>
            <a href="shop.html" class="cart-btn btn-lg">Shop Now</a>
        </div>
    </section>
	<!-- end shop banner -->


	<!-- logo carousel -->
	<div class="logo-carousel-section">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<div class="logo-carousel-inner">
						<div class="single-logo-item">
							<img src="resources/assets/img/logos/logo_vw2.png" alt="">
						</div>
						<div class="single-logo-item">
							<img src="resources/assets/img/logos/logo_nissan.png" alt="">
						</div>
						<div class="single-logo-item">
							<img src="resources/assets/img/company-logos/3.png" alt="">
						</div>
						<div class="single-logo-item">
							<img src="resources/assets/img/company-logos/4.png" alt="">
						</div>
						<div class="single-logo-item">
							<img src="resources/assets/img/company-logos/5.png" alt="">
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end logo carousel -->

	<!-- footer -->
	<%- include('fragmentos/footer') %>
	<!-- end footer -->
	
	<!--SCRIPTSSSSSS-->
	<%- include('fragmentos/scripts') %> 
	<!-- end SCRIPTSSSSSS-->
</body>
</html>